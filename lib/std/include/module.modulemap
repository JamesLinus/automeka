      explicit module uio {
        header "sys/uio.h"
        export *
      }
      explicit module msg {
        header "sys/msg.h"
        export *
      }
      explicit module un {
        header "sys/un.h"
        export *
      }
      explicit module mman {
        header "sys/mman.h"
        export *
      }
      explicit module select {
        header "sys/select.h"
        export *
      }
      explicit module statvfs {
