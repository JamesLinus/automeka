  explicit module istream {
    header "istream"
    requires cplusplus
    export *
  }
  explicit module ostream {
    header "ostream"
    requires cplusplus
    export *
  }
  explicit module iostream {
    header "iostream"
    requires cplusplus
    export ios
    export streambuf
    export istream
    export ostream
    export *
  }
  explicit module fstream {
    header "fstream"
    requires cplusplus
    export *
  }
  explicit module sstream {
    header "sstream"
    requires cplusplus
    export *
  }
  explicit module iomanip {
    header "iomanip"
    requires cplusplus
    export *
  }
  explicit module streambuf {
