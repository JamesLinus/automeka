    }
    explicit module search {
      header "search.h"
      export *
    }
    explicit module semaphore {
      header "semaphore.h"
      export *
    }
    explicit module spawn {
      header "spawn.h"
      export *
    }
    explicit module strings {
      header "strings.h"
      export *
    }
    explicit module stropts {
      header "stropts.h"
      export *
    }
    explicit module sys {
      explicit module wait {
        header "sys/wait.h"
        export *
      }
      explicit module types {
        header "sys/types.h"
        export *
      }
      explicit module socket {
        header "sys/socket.h"
        export *
      }
      explicit module shm {
        header "sys/shm.h"
        export *
      }
      explicit module utsname {
