        export *
      }
      explicit module stat {
        header "sys/stat.h"
        export *
      }
      explicit module ipc {
        header "sys/ipc.h"
        export *
      }
      explicit module sem {
        header "sys/sem.h"
        export *
      }
    }
    explicit module syslog {
      header "syslog.h"
      export *
    }
    explicit module tar {
      header "tar.h"
      export *
