    explicit module cpio {
      header "cpio.h"
      export *
    }
    explicit module dirent {
      header "dirent.h"
      export *
    }
    explicit module dlfcn {
      header "dlfcn.h"
      export *
      link "dl"
    }
    explicit module fcntl {
      header "fcntl.h"
      export *
    }
    explicit module fmtmsg {
      header "fmtmsg.h"
      export *
    }
    explicit module fnmatch {
      header "fnmatch.h"
