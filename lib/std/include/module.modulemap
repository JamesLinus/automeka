module stdlib [system] [extern_c] {
  textual header "stddef.h"
  header "stdlib.h"
  header "sys/types.h"
  header "sys/select.h"
  header "time.h"
  export *
}

module std [system] {
  // Utilities library
  explicit module cstdlib {
    header "cstdlib"
    header "stdlib.h"
    requires cplusplus
    export *
  }
  explicit module csignal {
    header "csignal"
    header "signal.h"
    requires cplusplus
    export *
  }
  explicit module csetjmp {
    header "csetjmp"
    header "setjmp.h"
    requires cplusplus
    export *
  }
  explicit module cstdarg {
    header "cstdarg"
    header "stdarg.h"
    requires cplusplus
    export *
  }
  explicit module typeinfo {
    header "typeinfo"
    requires cplusplus
    export *
  }
  explicit module typeindex {
    header "typeindex"
    requires cplusplus11
    export *
  }
  explicit module type_traits {
    header "type_traits"
    requires cplusplus11
    export *
  }
  explicit module bitset {
    header "bitset"
    requires cplusplus
    export string
    export iosfwd
    export *
  }
  explicit module functional {
    header "functional"
    requires cplusplus
    export *
  }
  explicit module utility {
    header "utility"
    requires cplusplus
    export initializer_list
    export string
    export *
  }
  explicit module ctime {
    header "ctime"
    header "time.h"
    requires cplusplus
    export *
  }
  explicit module chrono {
    header "chrono"
    requires cplusplus11
    export *
  }
  explicit module cstddef {
    textual header "cstddef"
    textual header "stddef.h"
    requires cplusplus
    export *
  }
  explicit module initializer_list {
    header "initializer_list"
    requires cplusplus11
    export *
  }
  explicit module tuple {
    header "tuple"
    requires cplusplus11
    export *
  }

  // Dynamic memory management
  explicit module new {
    header "new"
    requires cplusplus
    export *
  }
  explicit module memory {
    header "memory"
    requires cplusplus
    export *
  }
  explicit module scoped_allocator {
    header "scoped_allocator"
    requires cplusplus11
    export *
  }

  // Numeric limits
  explicit module climits {
    header "climits"
    header "limits.h"
    requires cplusplus
    export *
  }
  explicit module cfloat {
    header "cfloat"
    header "float.h"
    requires cplusplus
    export *
  }
  explicit module cstdint {
    header "cstdint"
    header "stdint.h"
    requires cplusplus11
    export *
  }
  explicit module cinttypes {
    header "cinttypes"
    header "inttypes.h"
    requires cplusplus11
    export *
  }
  explicit module limits {
    header "limits"
    requires cplusplus
    export *
  }

  // Error handling
  explicit module exception {
    header "exception"
    requires cplusplus
    export *
  }
  explicit module stdexcept {
    header "stdexcept"
    requires cplusplus
    export *
  }
  explicit module cassert {
    textual header "cassert"
    textual header "assert.h"
    requires cplusplus
    export *
  }
  explicit module system_error {
    header "system_error"
    requires cplusplus11
    export *
  }
  explicit module cerrno {
    header "cerrno"
    header "errno.h"
    requires cplusplus
    export *
  }

  // Strings library
  explicit module cctype {
    header "cctype"
    header "ctype.h"
    requires cplusplus
    export *
  }
  explicit module cwctype {
    header "cwctype"
    header "wctype.h"
    requires cplusplus
    export *
  }
  explicit module cstring {
    header "cstring"
    header "string.h"
    requires cplusplus
    export *
  }
  explicit module cwchar {
    header "cwchar"
    header "wchar.h"
    requires cplusplus
    export *
  }
  explicit module cuchar {
    header "cuchar"
    requires cplusplus11
    export *
  }
  explicit module string {
    header "string"
    requires cplusplus
    export initializer_list
    export *
  }

  // Containers library
  explicit module array {
    header "array"
    requires cplusplus11
    export initializer_list
    export *
  }
  explicit module vector {
    header "vector"
    requires cplusplus
    export initializer_list
    export memory
    export *
  }
  explicit module deque {
    header "deque"
    requires cplusplus
    export initializer_list
    export *
  }
  explicit module list {
    header "list"
    requires cplusplus
    export initializer_list
    export *
  }
  explicit module forward_list {
    header "forward_list"
    requires cplusplus11
    export initializer_list
    export *
  }
  explicit module set {
    header "set"
    requires cplusplus
    export initializer_list
    export memory
    export *
  }
  explicit module map {
    header "map"
    requires cplusplus
    export initializer_list
    export memory
    export set
    export *
  }
  explicit module unordered_set {
    header "unordered_set"
    requires cplusplus11
    export initializer_list
    export *
  }
  explicit module unordered_map {
    header "unordered_map"
    requires cplusplus11
    export initializer_list
    export unordered_set
    export *
  }
  explicit module stack {
    header "stack"
    requires cplusplus
    export initializer_list
    export *
  }
  explicit module queue {
    header "queue"
    requires cplusplus
    export initializer_list
    export *
  }

  // Algorithms library
  explicit module algorithm {
    header "algorithm"
    requires cplusplus
    export initializer_list
    export *
  }

  // Iterators library
  explicit module iterator {
    header "iterator"
    requires cplusplus
    export *
  }

  // Numerics library
  explicit module cmath {
    header "cmath"
    header "math.h"
    requires cplusplus
    export *
  }
  explicit module complex {
    header "complex"
    requires cplusplus
    export *
  }
  explicit module valarray {
    header "valarray"
    requires cplusplus
    export initializer_list
    export *
  }
  explicit module random {
    header "random"
    requires cplusplus11
    export initializer_list
    export *
  }
  explicit module numeric {
    header "numeric"
    requires cplusplus
    export *
  }
  explicit module ratio {
    header "ratio"
    requires cplusplus11
    export *
  }
  explicit module cfenv {
    header "cfenv"
    header "fenv.h"
    requires cplusplus11
    export *
