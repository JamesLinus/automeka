  // Regular Expressions library
  explicit module regex {
    header "regex"
    requires cplusplus11
    export initializer_list
    export *
  }

  // Atomic Operations library
  explicit module atomic {
    header "atomic"
    requires cplusplus11
    export *
  }

  // Thread support library
  explicit module thread {
    header "thread"
    requires cplusplus11
    export *
  }
  explicit module mutex {
    header "mutex"
    requires cplusplus11
    export *
  }
  explicit module shared_mutex {
    header "shared_mutex"
    requires cplusplus14
    export *
  }
  explicit module future {
    header "future"
    requires cplusplus11
    export *
  }
  explicit module condition_variable {
    header "condition_variable"
    requires cplusplus11
    export *
  }

  // C compatibility headers
  explicit module ciso646 {
    header "ciso646"
    header "iso646.h"
    requires cplusplus
    export *
  }
  explicit module ccomplex {
    header "ccomplex"
    header "complex.h"
    requires cplusplus11
    export complex
    export *
  }
  explicit module ctgmath {
    header "ctgmath"
    header "tgmath.h"
    requires cplusplus11
    export ccomplex
    export cmath
    export *
  }
  explicit module cstdalign {
    header "cstdalign"
    requires cplusplus11
    export *
  }
  explicit module cstdbool {
    header "cstdbool"
    header "stdbool.h"
    requires cplusplus11
    export *
